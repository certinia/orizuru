{
	"version": "0.2.0",
	"configurations": [{
		"type": "node",
		"request": "launch",
		"name": "Debug web example",
		"runtimeArgs": [
			"src/example/web.js"
		]
	}, {
		"type": "node",
		"request": "launch",
		"name": "Debug worker example 1",
		"runtimeArgs": [
			"src/example/worker1.js"
		]
	}, {
		"type": "node",
		"request": "launch",
		"name": "Debug worker example 2",
		"runtimeArgs": [
			"src/example/worker2.js"
		]
	}, {
		"type": "node",
		"request": "launch",
		"name": "Debug Tests",
		"program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
		"args": [
			"-u",
			"tdd",
			"--timeout",
			"999999",
			"--colors",
			"--recursive",
			"${workspaceRoot}/src/spec"
		],
		"internalConsoleOptions": "openOnSessionStart"
	}]
}