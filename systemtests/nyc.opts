{
	"all": true,
	"check-coverage": true,
	"lines": 80,
	"statements": 80,
	"functions": 80,
	"branches": 50,
	"require": [
		"ts-node/register"
	],
	"extension": [
		".ts"
	],
	"exclude": [
		"coverage",
		"coverage-systemtests",
		"dist",
		"doc",
		"**/*.d.ts",
		"test",
		"manualtests",
		"systemtests"
	],
	"reporter": [
		"lcov",
		"html",
		"text",
		"text-summary"
	],
	"sourceMap": true,
	"instrument": true
}
