{
  "name": "@financialforcedev/orizuru",
  "version": "5.1.2",
  "description": "Streamlined communication between Heroku dynos / other worker processes",
  "main": "src/lib/index.js",
  "scripts": {
    "doc": "jsdoc src/lib -r -d doc --readme readme.md",
    "pretest": "eslint src",
    "test": "nyc --exclude src/example mocha --recursive src/spec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/financialforcedev/orizuru"
  },
  "keywords": [
    "webhook",
    "web",
    "heroku",
    "worker",
    "orizuru",
    "dyno"
  ],
  "author": "financialforce",
  "license": "BSD-3-Clause",
  "dependencies": {
    "avsc": "5.1.1",
    "body-parser": "1.18.2",
    "debug": "3.1.0",
    "express": "4.16.2",
    "helmet": "3.9.0",
    "lodash": "4.17.4"
  },
  "devDependencies": {
    "@financialforcedev/eslint-config": "3.0.1",
    "app-root-path": "2.0.1",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "jsdoc": "3.5.5",
    "mocha": "4.0.1",
    "nyc": "11.3.0",
    "proxyquire": "1.8.0",
    "sinon": "4.1.3",
    "sinon-chai": "2.14.0",
    "supertest": "3.0.0"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "include": [
      "src"
    ],
    "reporter": [
      "lcov",
      "html",
      "text",
      "text-summary"
    ],
    "cache": true,
    "all": true
  }
}
