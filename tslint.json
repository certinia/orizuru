{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"indent": [
			true,
			"tabs",
			4
		],
		"quotemark": [
			true,
			"single"
		],
		"trailing-comma": [
			true,
			{
				"multiline": "never",
				"singleline": "never"
			}
		],
		"interface-name": false,
		"max-line-length": false,
		"no-empty-interface": false,
		"no-unused-expression": false,
		"no-unused-variable": true,
		"no-any": false,
		"no-namespace": [
			true,
			"allow-declarations"
		]
	},
	"rulesDirectory": []
}
